{
  "name": "@salt-ds/icons",
  "version": "1.7.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpmorganchase/salt-ds.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/jpmorganchase/salt-ds/issues",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./styles.css": "./dist/esm/index.css"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepare": "yarn clean && yarn node ./scripts/generateIcons.mjs '*.svg'",
    "clean": "rimraf ./src/components"
  },
  "dependencies": {
    "@salt-ds/styles": "workspace:^",
    "@salt-ds/window": "workspace:^",
    "clsx": "^2.0.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.0",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "glob": "^8.0.0",
    "mustache": "^4.2.0",
    "rimraf": "^4.4.0",
    "svgo": "^3.0.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "files": [
    "dist"
  ]
}
